{
  "track_name": "newworld",
  "track_randomized": true,
  "reward_type": "speed",
  "action_type": "continuous",
  "speed_limit": 2.0,

  "model": "stan_ppo",
  "policy": "actor_critic_pixels",
  "cuda": true,
  "seed": 1,

  "fixed_action_std": false,
  "hidden_size": 256,
  "recurring_cell": "none",

  "rollout_size": 128,
  "worker_count": 16,
  "mini_batch_count": 32,

  "learning_rate": 0.0002,
  "gamma": 0.99,
  "tau": 0.95,

  "ppo_epoch_count": 20,
  "ppo_clip": 0.2,

  "action_loss_coeff": 1.0,
  "value_loss_coeff": 0.5,
  "entropy_loss_coeff": 0.01,
  "auxiliary_loss_coeff": 0.5,

  "grad_norm_max": 0.0,

  "simulation_headless": true,
  "simulation_time_scale": 5.0,
  "simulation_step_interval": 0.20,
  "simulation_capture_frame_rate": 100
}
