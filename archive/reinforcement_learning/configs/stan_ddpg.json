  "actor_learning_rate": 0.0001,
  "critic_learning_rate": 0.001,
  "gamma": 0.99,
  "tau": 0.01,
  "epoch_count": 2,

  "grad_norm_max": 0.0,

  "simulation_headless": true,
  "simulation_time_scale": 4.0,
  "simulation_step_interval": 0.19,
  "simulation_capture_frame_rate": 200
}
